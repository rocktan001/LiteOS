menu "Agent Tiny Lwm2m Demo"

config LOSCFG_DEMOS_AGENT_TINY_LWM2M
    bool "Enable Agent Tiny Lwm2m Demo"
    select LOSCFG_COMPONENTS_CONNECTIVITY
    select LOSCFG_COMPONENTS_ATINY_LWM2M
    select LOSCFG_COMPONENTS_NET_LWIP
    default n
    help
      Answer y to enable agent tiny lwm2m demo.

endmenu
